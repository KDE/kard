<!DOCTYPE UI><UI version="3.3" stdsetdef="1">
<class>Theme</class>
<widget class="QWidget">
    <property name="name">
        <cstring>Theme</cstring>
    </property>
    <property name="geometry">
        <rect>
            <x>0</x>
            <y>0</y>
            <width>180</width>
            <height>221</height>
        </rect>
    </property>
    <grid>
        <property name="name">
            <cstring>unnamed</cstring>
