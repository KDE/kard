                            </property>
                            <property name="buttonGroupId">
                                <number>-1</number>
                            </property>
                        </widget>
                        <widget class="QRadioButton">
                            <property name="name">
                                <cstring>opposites</cstring>
                            </property>
                            <property name="text">
                                <string>Opposites</string>
                            </property>
                            <property name="buttonGroupId">
                                <number>-1</number>
                            </property>
                        </widget>
                        <widget class="QRadioButton">
